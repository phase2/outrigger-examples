{
  "env": {
    "browser": true
  },
  "globals": {
    "Drupal": true,
    "jQuery": true,
    "tinyMCE": true
  },
  "rules": {
    "eqeqeq": [2, "smart"],
    "guard-for-in": 2,
    "no-undef": 2,
    //"no-unused-vars": [2, {"vars": "local", "args": "none"}],
    "no-unused-vars": 0,
    "strict": 0,
    "new-cap": 0,
    "quotes": 0,
    "camelcase": 0,
    "no-underscore-dangle": 0,
    "no-new": 0,
    "no-alert": 0,
    "no-use-before-define": 0,
    "consistent-return": 0,
    "no-constant-condition": 0,
    "comma-dangle" : 2,
    "no-catch-shadow" : 2
  }
}
